<table width="100%" style="width:100%;mso-cellspacing:0cm;mso-yfti-tbllook:1184">
    <tbody>
    <tr style="mso-yfti-irow:0;mso-yfti-firstrow:yes">
        <td nowrap="" style="background:#1A237E;padding:3.0pt 6.0pt 6.0pt 12.0pt">
            <p class="MsoNormal">
                <b><span
                        style='font-size:13.5pt;font-family:"Segoe UI",sans-serif;mso-fareast-font-family:"Times New Roman";color:white'>Makves DCAP Оповещение:</span></b>
                <span style='font-size:13.5pt;font-family:"Segoe UI";,sans-serif;mso-fareast-font-family:"Times New Roman";color:white'>&nbsp; {{#data}}{{action}} {{when}} {{who}} на {{where}}{{/data}}
                    <o:p></o:p>
                </span>
            </p>
        </td>
    </tr>
    </tbody>
</table>
<div>
<pre style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{body}}</pre>
</div>
<table class="table table-responsive">
    <thead>
        <tr>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Свойство</th>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Значение</th>
        </tr>
    </thead>
    <tbody>
    {{#data}}
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Время</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{time_string}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>ID-события</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{event_id}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Категория</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{category}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Важность</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{severity}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Действие</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{action}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Кто</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{who}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Что</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{what}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Где</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{where}}</td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Поле 1</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'><pre>{{aux1}}</pre></td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Поле 2</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'><pre>{{aux2}}</pre></td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Регулируется стандартом</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>
                {{#compliance}}
                    <span style='font-size:10.0pt;font-family:"Segoe UI Semibold",sans-serif;color:red'>◉&nbsp;</span>
                    {{name}}
                {{/compliance}}
            </td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="2">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
        <tr>
            <td style='padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E;font-family:"Segoe UI Semibold",sans-serif;'>Содержимое</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>
                <pre>{{contents}}</pre>

            </td>
        </tr>
    {{/data}}
</table>

<table border="0" cellspacing="0" cellpadding="0" width="100%" style="width:100.0%">
    <tbody>
    <tr>
        <td style="background:white;padding:0cm" >
            <div align="center" style="text-align:center">
                <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;color:black'>
                    <hr size="1" width="100%" noshade="" style="color:#1171B7">
                </span>
            </div>
        </td>
    </tr>
    <tr>
        <td style="background:white;padding:0cm 12.0pt 0cm 12.0pt">
            <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;color:black'>Оповещение создано {{report_time}}</span>
        </td>
    </tr>
    </tbody>
</table>
