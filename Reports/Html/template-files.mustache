<table width="100%" style="width:100%;mso-cellspacing:0cm;mso-yfti-tbllook:1184">
    <tbody>
    <tr style="mso-yfti-irow:0;mso-yfti-firstrow:yes">
        <td nowrap="" style="background:#1A237E;padding:3.0pt 6.0pt 6.0pt 12.0pt">
            <p class="MsoNormal">
                <b><span style='font-size:13.5pt;font-family:"Segoe UI",sans-serif;mso-fareast-font-family:"Times New Roman";color:white'>Makves DCAP:</span></b>
                <span style='font-size:13.5pt;font-family:"Segoe UI";,sans-serif;mso-fareast-font-family:"Times New Roman";color:white'>&nbsp;{{title}}<o:p></o:p>
                </span>
            </p>
        </td>
    </tr>
    </tbody>
</table>

<table class="table table-responsive">
    <thead>
        <tr>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Каталог</th>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Имя</th>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Тип</th>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Размер</th>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Время доступа</th>
            <th style="background:#BBDEFB;padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Время изменения</th>
            <th style="width:30.0%;background:#BBDEFB; padding:3.0pt 12.0pt 4.5pt 12.0pt;color:#1A237E">Детали</th>
        </tr>
    </thead>
    <tbody>
    {{#items}}
        <tr>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{folder}}</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI Semibold",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{name}}</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{type}}</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{size_string}}</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{accessed_at_string}}</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>{{modified_at_string}}</td>
            <td style='font-size: 10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>
                {{#score}}
                    <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>Риск:<span style='font-size:10.0pt;font-family:"Segoe UI Semibold",sans-serif;color:{{score_color}}'>■&nbsp;</span>{{score}}
                    </span>
                {{/score}}
                {{#compliance}}
                    <span style='font-size:10.0pt;font-family:"Segoe UI Semibold",sans-serif;color:red'>◉&nbsp;</span>
                    {{name}}
                {{/compliance}}
                {{#not_inherited_ace_count}}
                    <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>Не наследуемых прав:&nbsp;<span style='font-family:"Segoe UI Semibold"'>{{not_inherited_ace_count}}</span>&nbsp;</span>
                {{/not_inherited_ace_count}}
                {{#events_counter}}
                    <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>Событий:&nbsp;<span style='font-family:"Segoe UI Semibold"'>{{events_counter}}</span>&nbsp;</span>
                {{/events_counter}}
                {{#counter}}
                    <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;padding:0cm 12.0pt 0cm 12.0pt'>Дубликатов:&nbsp;<span style='font-family:"Segoe UI Semibold"'>{{counter}}</span>&nbsp;</span>
                {{/counter}}
            </td>
            </td>
        </tr>
        <tr style="mso-yfti-irow:10">
            <td style="padding:0cm 0cm 0cm 0cm" colspan="11">
                <hr size="1" width="100%" noshade="" style="color:#D8D8D8" align="center">
            </td>
        </tr>
    {{/items}}
</table>

<table border="0" cellspacing="0" cellpadding="0" width="100%" style="width:100.0%">
    <tbody>
    <tr>
        <td style="background:white;padding:0cm" >
            <div align="center" style="text-align:center">
                <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;color:black'>
                    <hr size="1" width="100%" noshade="" style="color:#1171B7">
                </span>
            </div>
        </td>
    </tr>
    <tr>
        <td style="background:white;padding:0cm 12.0pt 0cm 12.0pt">
            <span style='font-size:10.0pt;font-family:"Segoe UI",sans-serif;color:black'>Отчет создан {{report_time}}</span>
        </td>
    </tr>
    </tbody>
</table>